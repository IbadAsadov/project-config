{
    "compilerOptions": {
        "target": "esnext",
        "allowJs": true,
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
